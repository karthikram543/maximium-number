public class max {
	public static void main (String[]args) {
		 int number1=50;
		 int number2=100;
		 
		 
		 if(number1>number2) {
			 System.out.println("maximium number is"+number1);
		 }
		 if(number2>number1) {
			 System.out.println("maximium number is:"+ number2);
		 }
		 if(number1 == number2) {
			 System.out.println("both are equal");
		 }
		 
}

}
	  
	   
	

	
	
